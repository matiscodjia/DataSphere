    "def fibonacci(n):\n",
    "    numbers = []\n",
    "    a  = 0\n",
    "    b = 1\n",
    "    while a < n:\n",
    "        numbers.append(a)\n",
    "        a, b = b, a+b\n",
    "    print(numbers)      \n",
    "\n",
    "    "
   ]
